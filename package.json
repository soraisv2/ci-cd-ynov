{
  "name": "ci-cd-ynov",
  "version": "0.1.2",
  "private": false,
  "homepage": "https://soraisv2.github.io/ci-cd-ynov/",
  "dependencies": {
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^14.5.2",
    "axios": "^1.6.7",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.21.3",
    "react-scripts": "5.0.1",
    "use": "^3.1.1",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "build-npm": "NODE_ENV=production && rm -rf dist && mkdir dist && npx babel src --out-dir dist --copy-files",
    "test": "react-scripts test  --coverage --collectCoverageFrom=src/*/js* --collectCoverageFrom=!src/reportWebVitals.js --collectCoverageFrom=!src/index.js --watchAll=false --setupFiles=./.jest/setEnvVars.js",
    "eject": "react-scripts eject",
    "jsdoc": "jsdoc -R ./README.md -c ./jsdoc.config.json -r -d ./public/docs",
    "test:coverage": "react-scripts test --collectCoverageFrom=src/*/js* --collectCoverageFrom=!src/reportWebVitals.js --collectCoverageFrom=!src/index.js --coverage --watchAll=false",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build",
    "cypress": "cypress open"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/cli": "^7.25.7",
    "@babel/core": "^7.25.8",
    "@babel/plugin-proposal-private-property-in-object": "^7.21.11",
    "@babel/plugin-transform-react-jsx": "^7.25.7",
    "@babel/preset-env": "^7.23.9",
    "cypress": "^13.6.6",
    "gh-pages": "^6.3.0",
    "jsdoc": "^4.0.2",
    "react-test-renderer": "^18.2.0"
  },
  "jest": {
    "transformIgnorePatterns": [
      "node_modules/(?!axios)/"
    ]
  }
}
